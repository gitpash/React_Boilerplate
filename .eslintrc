{
  "extends": [
    "kentcdodds"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    "linebreak-style": 0,
    "import/no-unassigned-import": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "class-methods-use-this": 0,
    "no-invalid-this": 0,
    "babel/no-invalid-this": 2,
    "no-unused-vars": 0,
    "valid-jsdoc": 0,
    "no-console": 0,
  }
}
